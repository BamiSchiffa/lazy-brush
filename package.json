  "version": "0.0.1",
  "description": "Lazy brush library that allows for smooth drawing using the mouse.",
  "main": "lib/index.js",
  "keywords": [
    "brush",
    "mouse",
    "lazy",
    "canvas",
    "drawing"
  ],
  "repository": "https://github.com/dulnan/lazy-brush",
